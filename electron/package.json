{
  "name": "switchable",
  "version": "0.4.0",
  "description": "Create amount of daily plans, then quickly switch from one to another. ",
  "main": "index.js",
  "scripts": {
    "electron:start": "electron ./",
    "e": "yarn run electron:start",
    "electron:mac": "electron-builder build --mac --x64",
    "m": "yarn run electron:mac"
  },
  "dependencies": {
    "@capacitor/electron": "^1.1.0",
    "electron-is-dev": "^1.1.0"
  },
  "devDependencies": {
    "electron": "9.4.0",
    "electron-builder": "^21.0.15"
  },
  "keywords": [
    "capacitor",
    "electron"
  ],
  "author": "",
  "license": "MPL-2.0",
  "build": {
    "appId": "com.scrisstudio.switchasble",
    "productName": "Scris Switchable",
    "copyright": "Copyright Â© 2019 Tianze Ds Qiu from Scris Studio.",
    "mac": {
      "category": "public.app-category.productivity",
      "target": ["dmg"],
      "icon": "favicon.icns",
      "type": "distribution"
    },
    "win": {
      "target": ["zip"],
      "icon": "favicon.ico"
    }
  }
